<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lubna.mytenth" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".myList"
            android:label="@string/title_activity_my_list"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".group_list"
            android:label="@string/title_activity_group_list"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".go_shopping"
            android:label="@string/title_activity_go_shopping"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".myList2"
            android:label="@string/title_activity_my_list2"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".chat_message"
            android:label="@string/title_activity_chat_message"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
